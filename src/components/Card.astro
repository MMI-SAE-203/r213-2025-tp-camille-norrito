---
const { nomMaison, imgUrl } = Astro.props;
console.log("imgUrl=", imgUrl);
---

<div
    class="bg-zinc-100 text-stone-700 border border-stone-500 rounded-lg shadow-lg p-6 flex flex-col items-center w-80"
>
    <img
        src={imgUrl}
        alt={nomMaison}
        class="w-full h-40 object-cover mb-4 rounded-md"
    />
    <h2 class="text-2xl font-bold text-center">{nomMaison}</h2>
</div>
